-- A simple storage program

signature SimpleStorage {
    storage storedData : UInt;

	constructor c : void -> void;
	method set : (UInt) -> void;
}

constructor c (){
	storage
	returns void;
}
method set(x: UInt) {
	guard{
		x > 0;
	}
	storage{
    	storedData |-> x;
	}
	effects{}
	returns void;
}

